<div class="page-header">
  <h1>Cats!</h1>
</div>

<table class="table">
  <thead>
    <tr>
      <th>
        Name
      </th>
      <th>

      </th>
    </tr>
  </thead>
  <tbody>
    <% @cats.each do |cat| %>
      <tr>
        <td>
          <%= link_to cat.name, cat_path(cat) %>
        </td>
        <td>
          <% cat.toys.each do |toy| %>
            <ul>
            <li><%= toy.name %></li> 
            </ul>


          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
